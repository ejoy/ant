
import_feature "ant.shadow_bounding|scene_bounding"

import_feature "ant.landform"
import_feature "ant.grid"
import_feature "ant.animation"

system "init_system"
    .implement "init_system.lua"

pipeline "init"
    .stage "init"
    .stage "post_init"

pipeline "exit"
    .stage "exit"

pipeline "update"
    .stage "timer"
    .stage "start_frame"
    .stage "data_changed"
    .stage  "widget"
    .pipeline "sky"
    .pipeline "animation"
    .pipeline "scene"
    .pipeline "camera"
    .pipeline "render"
    .stage "end_frame"
    .stage "final"
